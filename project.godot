MusicManager="*res://autoloads/audio/music_manager.gd"
SoundManager="*res://autoloads/audio/sound_manager.gd"
Dialogic="*res://addons/dialogic/Core/DialogicGameHandler.gd"

[dialogic]

directories/dch_directory={
"hungry_kid": "res://mechanics/dialogic/characters/hungry_kid.dch"
}
directories/dtl_directory={}

[display]

window/size/resizable=false
window/stretch/mode="viewport"

[editor_plugins]

enabled=PackedStringArray("res://addons/Todo_Manager/plugin.cfg", "res://addons/dialogic/plugin.cfg")

[file_customization]

folder_colors={
"res://addons/": "blue",
"res://assets/": "pink",
"res://autoloads/": "yellow",
"res://mechanics/": "red",
"res://ui/": "green",
"res://utility/": "purple"
